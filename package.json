    "keytar": "^7.7.0",
    "mv": "2.1.1",
    "ncp": "~2.0.0",
    "npm": "https://github.com/pulsar-edit/npm-cli/releases/download/v6.14.19-pulsar1-1/npm-6.14.19-pulsar1-1.tgz",
    "open": "7.3.0",
    "plist": "3",
    "read": "~1.0.7",
    "request": "^2.88.2",
    "season": "^6.0.2",
